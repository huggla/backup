recursiveDeleteFileDuplicates(){
   local -
   local relPath=""
   local backupDir=""
   local latestDir=""
   if [ -z "$3" ]
   then
      backupDir=$1
      latestDir=$2
   else
      relPath=$1
      backupDir=$2
      latestDir=$3
   fi
   local file=""
   local fileName=""
   local newRelPath=""
   set +f
   for file in "$backupDir/$relPath"*
   do
      fileName="$(/usr/bin/basename "$file")"
      if [ -z "$relPath" ]
      then
         newRelPath=$fileName
      else
         newRelPath="$relPath/$fileName"
      fi
      if [ -d "$file" ] && [ "$file" != "." ] && [ "$file" != ".." ]
      then
         recursiveDeleteFileDuplicates $newRelPath $backupDir $latestDir
      elif [ -f "$file" ] && [ -f "$latestDir/$newRelPath" ]
      then
         if [ ! "$(/usr/bin/cmp "$file" "$latestDir/$newRelPath")" ]
         then
            /bin/rm "$latestDir/$newRelPath"
         fi
      fi
   done
}
